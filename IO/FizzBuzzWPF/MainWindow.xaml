<Window x:Class="FizzBuzzWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition Width="100"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <Button Content="Start" Click="ButtonBase_OnClick"></Button>
            <ListBox Name="GameListBox"></ListBox>
        </StackPanel>
        <StackPanel Grid.Column="1">
            <TextBox Name="StartRangeTextBox">0</TextBox>
            <TextBox Name="EndRangeTextBox">50</TextBox>
        </StackPanel>

    </Grid>
</Window>
